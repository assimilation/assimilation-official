  "nist_V-38484": {
	"category": "security",
        "rule": "MUST(IN($PrintLastLog, \"yes\", True))"
	},
  "nist_V-38607": {
	"category": "security",
        "rule": "MUST(IN($Protocol, 2))"
	},
  "nist_V-38608": {
	"category": "security",
        "rule": "MUST(LE($ClientAliveInterval, 900))"
	},
  "nist_V-38610": {
	"category": "security",
        "rule": "MUST(EQ($ClientAliveCountMax, 0))"
	},
  "nist_V-38611": {
	"category": "security",
        "rule": "MUST(IN($IgnoreRhosts, \"yes\", True))"
	},
  "nist_V-38612": {
	"category": "security",
        "rule": "IN($HostbasedAuthentication, \"no\", False))"
	},
  "nist_V-38613": {
	"category": "security",
        "rule": "MUST(IN($PermitRootLogin, \"no\", False))"
	},
  "nist_V-38614": {
	"category": "security",
        "rule": "IN($PermitEmptyPasswords, \"no\", False)"
	},
  "nist_V-38615": {
	"category": "security",
        "rule": "MUST(EQ($Banner, \"/etc/issue\"))"
	},
  "nist_V-38616": {
	"category": "security",
        "rule": "MUST(IN($PermitUserEnvironment, \"no\", False))"
	},
  "nist_V-38617": {
	"category": "security",
	"rule": "MUST(IN($Ciphers, aes128-ctr,aes192-ctr,aes256-ctr,aes128-cbc,3des-cbc,aes192-cbc,aes256-cbc))"
	}

}
